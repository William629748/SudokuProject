<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demosudoku.controller.SudokuHelpController">
   <children>
      <ImageView fitHeight="539.0" fitWidth="695.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Images/HelpView.png" />
         </image>
      </ImageView>
      <Label layoutX="129.0" layoutY="211.0" prefHeight="82.0" prefWidth="267.0" text="Llena la cuadricula solamente &#10;con numeros del 1 al 6">
         <font>
            <Font name="Bebas Neue Regular" size="25.0" />
         </font>
      </Label>
      <Label layoutX="136.0" layoutY="305.0" prefHeight="82.0" prefWidth="267.0" text="No repita numeros en filas &#10;ni en columnas">
         <font>
            <Font name="Bebas Neue Regular" size="25.0" />
         </font>
      </Label>
      <Label layoutX="136.0" layoutY="386.0" prefHeight="82.0" prefWidth="267.0" text="No repita numeros en los&#10;bloques 2x3&#10;">
         <font>
            <Font name="Bebas Neue Regular" size="25.0" />
         </font>
      </Label>
      <ImageView fx:id="MenuImage" fitHeight="150.0" fitWidth="200.0" layoutX="437.0" layoutY="270.0" onMouseClicked="#handleMenuClicked" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Images/Cerrar-removebg-preview.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
